<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              android:background="@color/Black">

    <com.Wonderland.graphicObjects.GroupActionBar
        xmlns:custom="http://schemas.android.com/apk/res/com.Wonderland.main"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar"
        custom:setText="@string/app_name"
        custom:showBackButton="false"
        android:id="@+id/view"/>

    <com.Wonderland.graphicObjects.MyImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:src="@drawable/home"
        android:id="@+id/home"/>
</LinearLayout>

